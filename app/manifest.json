{
  "name": "Weather",
  "version": "3.5.7",
  "description": "The best way to see the weather right in your browser. Easier than looking outside!",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "scripts": ["vendor/js/jquery-2.1.4.min.js", "js/ga.js", "js/init.js", "js/popup.js", "js/background.js"]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "browser_action" : {
	"default_icon": { "38": "img/icon38.png" },
    "default_title": "Weather",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "permissions": [ "alarms", "storage", "geolocation", "*://*.forecast.io/*", "*://*.timleland.com/*", "notifications"]
}
