{
  "name": "Weather",
  "version": "4.0.3",
  "description": "The best way to see the weather right in your browser. Easier than looking outside!",

  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action" : {
	"default_icon": { "38": "img/icon38.png" },
    "default_title": "Weather",
    "default_popup": "popup.html"
  },

  "manifest_version": 2,
  "permissions": [ "alarms", "storage", "*://*.timleland.com/*", "notifications"]

}
